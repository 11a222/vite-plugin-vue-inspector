{
  "name": "vite-plugin-vue-inspector",
  "version": "2.0.4",
  "description": "jump to local IDE source code while click the element of browser automatically.",
  "keywords": [
    "vue",
    "vite",
    "vscode",
    "vite-plugin",
    "inspector",
    "debug"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webfansplz/vite-plugin-vue-inspector.git"
  },
  "author": "webfansplz",
  "files": [
    "dist",
    "src/load.js",
    "src/Overlay.vue"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./overlay": {
      "import": "./dist/InspectorOverlay.vue",
      "require": "./dist/InspectorOverlay.vue"
    },
    "./overlay.vue": {
      "import": "./dist/InspectorOverlay.vue",
      "require": "./dist/InspectorOverlay.vue"
    },
    "./src/*": "./src/*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "scripts": {
    "lint": "eslint --fix --ext .js,.ts,.vue .",
    "update-version": "esmo scripts/update-version.ts",
    "copy-overlay": "esno scripts/copy-overlay.ts",
    "build": "tsup src/index.ts --format cjs,esm --dts && pnpm run copy-overlay"
  },
  "bugs": {
    "url": "https://github.com/webfansplz/vite-plugin-vue-inspector/issues"
  },
  "homepage": "https://github.com/webfansplz/vite-plugin-vue-inspector#readme",
  "peerDependencies": {
    "vite": "^3.0.0-0 || ^4.0.0-0"
  },
  "dependencies": {
    "@babel/core": "^7.20.7",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-typescript": "^7.20.7",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/compiler-dom": "^3.2.45",
    "esno": "^0.16.3",
    "kolorist": "^1.6.0",
    "magic-string": "^0.27.0",
    "shell-quote": "^1.7.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.0",
    "@types/babel__core": "^7.1.20",
    "eslint": "^8.31.0",
    "esmo": "^0.16.3",
    "fs-extra": "^11.1.0",
    "fsxx": "^0.1.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vue": "^3.2.45"
  }
}
